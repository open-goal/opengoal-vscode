{
  "comments": {
    "lineComment": ";;",
    "blockComment": ["#|", "|#"]
  },
  // symbols used as brackets
  "brackets": [["(", ")"]],
  "surroundingPairs": [
    ["(", ")"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^; \\.function.*",
      "end": "^;; \\.endfunction\\b"
    }
  },
  "wordPattern": "[^\\s,\\(\\)]+"
}
